<div class="top-right links">
    {% if app.user %}
        {% if not app.user.isAdmin %}
            <a href="{{ path('welcome') }}">Home</a>
        {% else %}
            <a href="{{ path('welcome') }}">Admin</a>
        {% endif %}
        <a href="{{ path('logout') }}">Logout</a>
    {% else %}
        <a href="{{ path('login') }}">Login</a>
        <a href="{{ path('register') }}">Register</a>
    {% endif %}
</div>
